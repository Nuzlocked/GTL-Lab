{
  "buildCommand": "npm run build",
  "outputDirectory": "build",
  "devCommand": "npm start",
  "installCommand": "npm install",
  "framework": "create-react-app",
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.gtl-lab.com/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "gtl-lab.com"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/:path*",
      "destination": "/index.html"
    }
  ]
} 